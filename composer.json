{
  "name": "codefarm/grabber",
  "type": "library",
  "description": "customizable package to grab data from webpage using php",
  "keywords": [
    "php",
    "scrape",
    "laravel",
    "HtmlParser"
  ],
  "authors": [
    {
      "name": "codefarm",
      "email": "codefarm.bs@gmail.com",
      "homepage": "https://percept.ir/@codefarm"
    }
  ],
  "homepage": "https://github.com/codefarm-bs/Grabber",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "mockery/mockery": "^1.4"
  },
  "require-dev": {
    "orchestra/testbench": "^6.23",
    "laravel/legacy-factories": "^1.1"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "autoload": {
    "psr-4": {
      "codefarm\\Grabber\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "codefarm\\Grabber\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "codefarm\\Grabber\\BaseServiceProvider"
      ]
    }
  }
}
